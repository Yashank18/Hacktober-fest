
import java.util.Scanner; public class Main { public static int trailingZeros(long n) { if(n == 1) { return 0; } int count = 0, current = 5; while(current <= n) { count += n / current; current *= 5; } return count; } public static void main(String[] args) { Scanner s = new Scanner(System.in); long n = s.nextLong(); int m = s.nextInt(); long start = 1, end = n, mid = 0; boolean flag = false; while(start <= end) { mid = (start + end) / 2; int count = trailingZeros(mid); if(count < m) { start = mid + 1; } else if(count > m) { end = mid - 1; } else { flag = true; break; } } if(!flag) { return; } System.out.println(mid); long left = mid - 1, right = mid + 1; while(left >= 1) { int count = trailingZeros(left); if(count == m) { System.out.println(left); left--; } else { break; } } while(right <= n) { int count = trailingZeros(right); if(count == m) { System.out.println(right); right++; } else { break; } } } }
