
#include<iostream> using namespace std; #include <vector> #include <map> #include <cstring> #include <algorithm> #include <set> using namespace std; #define fs first #define sc second #define MAX 100000 #define pb push_back #define mp make_pair #define INF (1LL<<62) #define MOD 1000000007 #define MAXN 1000000 struct{ set<int> v; } arr[30][30]; int main() { string s1,s2; int k; cin>>s1>>s2; cin >> k; for(int i=0;i<s1.length();i++) { if(s1[i]!=s2[i]) { arr[s1[i]-'a'][s2[i]-'a'].v.insert(i); } } int required_swaps=0; for(int i=0;i<s1.length();i++) { if(s1[i]!=s2[i]) { if(arr[s2[i]-'a'][s1[i]-'a'].v.size()!=0) { auto it1=arr[s2[i]-'a'][s1[i]-'a'].v.end(); it1--; int d=*it1; arr[s2[i]-'a'][s1[i]-'a'].v.erase(it1); auto it =arr[s1[i]-'a'][s2[i]-'a'].v.find(i); arr[s1[i]-'a'][s2[i]-'a'].v.erase(it); swap(s1[i],s1[d]); required_swaps++; } else { for(int j=0;j<26;j++) { if(arr[s2[i]-'a'][j].v.size()!=0) { auto it1=arr[s2[i]-'a'][j].v.end(); it1--; int d=*it1; arr[s2[i]-'a'][j].v.erase(it1); auto it =arr[s1[i]-'a'][s2[i]-'a'].v.find(i); arr[s1[i]-'a'][s2[i]-'a'].v.erase(it); arr[s1[i]-'a'][j].v.insert(d); swap(s1[i],s1[d]); required_swaps++; break; } } } } } //cout<<required_swaps<<endl; if(required_swaps <= k) { cout << "yes"; } else { cout << "no"; } return 0; }
